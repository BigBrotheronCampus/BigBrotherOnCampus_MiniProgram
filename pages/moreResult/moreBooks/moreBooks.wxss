  flex-direction: row;
  width: 100%;
  font-size:40rpx;
  background-color:  #cadbf9;
  margin-top: 5rpx;
  align-items: center;
}
.navbar .item{
  width: 33.33%;
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: white;
}
.navbar .active{
