.table{
  border: 5rpx solid #ffffff;
  color:#779dce;
  background: #f1f0fe;
  width: 100%;
  
}

.tableTitle{
  display: flex;
  justify-content: center;
  color: #de979b;
  font-size: 50rpx;
  height: 100rpx;
  align-items: center;
}

.header{
  display: flex;
  width: 100%;
  height:100rpx;
}

.headerColumn{
  border-top: 3px solid #ffffff;
  border-right: 3px solid #ffffff;
  flex-grow: 1;
  font-size: 40rpx; 
  height: 100rpx;
  width:33.33%;
  line-height: 100rpx;
  text-align: center;
}

.row{
  display: flex;
  width: 100%;
  height:100rpx;
}

.dd{
  border-top: 5px solid #ffffff;
  border-right: 5px solid #ffffff;
  flex-grow: 1;
  font-size: 40rpx;
  height: 100rpx;
  width:33.33%;
  line-height: 100rpx;
  text-align: center;
  word-break:break-all;
}